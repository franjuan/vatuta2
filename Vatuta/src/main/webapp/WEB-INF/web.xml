<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Vatuta</display-name>
  <servlet>
  	<servlet-name>VatutaServlet</servlet-name>
  	<display-name>VatutaServlet</display-name>
  	<description>Sample Servlet</description>
  	<servlet-class>com.example.myproject.VatutaServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>VatutaServlet</servlet-name>
  	<url-pattern>/vatuta</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>
